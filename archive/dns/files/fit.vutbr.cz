;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	fit.vutbr.cz. root.fit.vutbr.cz. (
			      1		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@	IN	NS	ns.fit.vutbr.cz.
ns	IN	A	192.168.1.1
www     IN      A       192.168.1.1
